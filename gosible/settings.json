{
    "iredmail_backend": "openldap",
    "iredmail_backend": "pgsql",
    "iredmail_backend": "mariadb",
    "ldap_suffix": "dc=a,dc=io",
    "bind_upstream_servers": ["8.8.8.8", "8.8.4.4"],
    "//use_nfs_storage": true,
    "nfs_export_dir": "/var/vmail",
    "nfs_mount_point": "/var/vmail",
    "storage_base_dir": "/var/vmail",
    "first_mail_domain": "a.io",
    "//use_firewall": true,
    "//use_ntp": true,
    "//use_bind": true,
    "use_antispam": true,
    "use_nginx": true,
    "use_phpfpm": true,
    "use_roundcube": true,
    "use_autoconfig": true,
    "use_iredapd": true,
    "use_sogo": true,
    "use_fail2ban": true,
    "use_iredadmin": true,
    "use_netdata": true,
    "use_adminer": true,
    "use_prosody": true,
    "use_backup": true,
    "//homepage_application": "sogo",
    "homepage_application": "roundcube",
    "//iredadmin_pro_license_key": "",
    "dovecot_expunge_trash_days": 60,
    "dovecot_expunge_junk_days": 60,
    "dovecot_enable_last_login": true,
    "dovecot_enable_fts": true,
    "sogo_prefork_processes": 2,
    "sogo_allow_cross_domain_user_query": true,
    "message_size_limit_in_mb": 50,
    "web_upload_size_limit_in_mb": 30,
    "postfix_enable_srs": true,
    "postfix_enable_auth_on_port_25": true,
    "//antispam_disable_spam_scanning": true,
    "//antispam_disable_virus_scanning": true,
    "iredadmin_pro_enable_restful_api": true,
    "trusted_clients": ["192.168.1.1", "172.0.0.0/8", "172.16.0.0/12", "2a01:79a:409:120::/64"],
    "fail2ban_extra_ignoreip": ["192.168.1.2", "192.168.0.0/24"],
    "fail2ban_store_banned_ip_in_db": true,
    "mysql_root_password": "www",
    "antispam_ban_file_types": ["doc", "docx"],
    "first_domain_admin_password": "www",
    "//iredapd_download_url": "http://172.16.100.1/~zhb/mirrors/misc/iRedAPD-{{ iredapd_version }}.tar.gz",
    "//mlmmjadmin_download_url": "http://172.16.100.1/~zhb/mirrors/misc/mlmmjadmin-{{ mlmmjadmin_version }}.tar.gz",
    "//adminer_download_url": "http://172.16.100.1/~zhb/mirrors/misc/adminer-{{ adminer_version }}.php",
    "//netdata_download_url": "http://172.16.100.1/~zhb/mirrors/misc/netdata-v{{ netdata_version }}.gz.run",
    "//roundcube_download_url": "http://172.16.100.1/~zhb/mirrors/misc/roundcubemail-{{ roundcube_version }}-complete.tar.gz",
    "//iredadmin_ose_download_url": "http://172.16.100.1/~zhb/mirrors/misc/iRedAdmin-{{ iredadmin_ose_version }}.tar.gz"
}
