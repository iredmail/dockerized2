[postfix]
backend = polling
journalmatch =
enabled     = true
filter      = postfix.iredmail
logpath     = /var/log/mail.log tail
action      = iptables-multiport[name=postfix, port="25,587,465,2525", protocol=tcp]
              banned_db[name=postfix, port="25,587,465,2525", protocol=tcp]
